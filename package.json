    "react-addons-test-utils": "^15.4.2",
    "react-scripts": "0.9.0",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "chai": "^3.5.0",
    "enzyme": "^2.7.1",
    "i18next": "^7.1.1",
    "react": "^15.4.2",
    "react-addons-update": "^15.4.2",
