{
  "presets": [
    "module:metro-react-native-babel-preset",
    "module:react-native-dotenv"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "ui": "./src/components/ui"
        },
        "cwd": "babelrc",
        "extensions": [
          ".js",
          ".ios.js",
          ".android.js"
        ],
        "root": ["./src"]
      }
    ]
  ]
}
